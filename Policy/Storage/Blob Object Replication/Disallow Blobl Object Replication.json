{
    "mode": "All",
    "policyRule": {
      "if": {
        "field": "Microsoft.Storage/storageAccounts/blobServices/default.isVersioningEnabled",
        "equals": "true"
      },
      "then": {
        "effect": "[parameters('effect')]"
      }
    },
    "parameters": {
      "effect": {
        "type": "String",
        "metadata": {
          "displayName": "Effect",
          "description": "The effect determines what happens when the policy rule is evaluated to match"
        },
        "allowedValues": [
          "Audit",
          "Deny",
          "Disabled"
        ],
        "defaultValue": "Deny"
      }
    }
  }